public class Ave {
    public void comer() {
        System.out.println("Ave comendo.");
    }
}


public interface AveVoadora {
    void voar();
}

public class Pinguim extends Ave {
    public void nadar() {
        System.out.println("Pinguim nadando rapidamente!");
    }
}

public class Pardal extends Ave implements AveVoadora {
    @Override
    public void voar() {
        System.out.println("Pardal voando.");
    }
}
